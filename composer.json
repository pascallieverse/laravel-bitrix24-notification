{
  "name": "pascallieverse/laravel-bitrix24-notification",
  "description": "Package for Laravel notifications to Bitrix24",
  "keywords": ["laravel", "notification", "Bitrix24"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal Lieverse",
      "email": "pascallieverse@live.nl"
    }
  ],
  "require": {
    "php": "^8.0.1",
    "illuminate/notifications": "^9.0|^10.0",
    "illuminate/support": "^9.0|^10.0",
    "illuminate/queue": "^9.0|^10.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^10"
  },
  "autoload": {
    "classmap": [],
    "psr-4": {
      "PascalLieverse\\Bitrix24\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PascalLieverse\\Bitrix24\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PascalLieverse\\Bitrix24\\Bitrix24ServiceProvider"
      ]
    }
  }
}
